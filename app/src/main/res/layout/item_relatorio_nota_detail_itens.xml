<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingBottom="@dimen/list_item_horizontal_padding"
    android:paddingEnd="@dimen/list_item_vertical_padding"
    android:paddingStart="@dimen/list_item_vertical_padding"
    android:paddingTop="@dimen/list_item_horizontal_padding">

    <TextView
        android:id="@+id/item_relatorio_nota_detail_nome_text_view"
        style="@style/ListItemPrimaryText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="2" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                style="@style/ListItemSecondaryText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/form_label_margin_horizontal"
                android:layout_marginTop="@dimen/form_label_margin_vertical"
                android:text="@string/SKU"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/item_relatorio_nota_detail_sku_text_view"
                style="@style/ListItemSecondaryText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/form_label_margin_horizontal"
                android:layout_marginTop="@dimen/form_label_margin_vertical"
                android:layout_weight="2"
                android:maxLines="1" />

            <TextView
                style="@style/ListItemSecondaryText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/form_label_margin_horizontal"
                android:layout_marginTop="@dimen/form_label_margin_vertical"
                android:maxLines="1"
                android:text="@string/qtd"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/item_relatorio_nota_detail_qtd_text_view"
                style="@style/ListItemSecondaryText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/form_label_margin_vertical"
                android:layout_weight="1"
                android:maxLines="1" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:text="@string/icmsst"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_icmsst_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1"
                    android:text="@string/vlricms"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_vlricms_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:text="@string/vlripi"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_vlripi_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1"
                    android:text="@string/vlrdesc"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_vlrdesc_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />
            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:text="@string/vlrunit"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_vlruni_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/form_label_margin_horizontal"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1"
                    android:text="@string/vlrtotal"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/item_relatorio_nota_detail_vlrtotal_text_view"
                    style="@style/ListItemSecondaryText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/form_label_margin_vertical"
                    android:maxLines="1" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>